@model Absence.Models.T_Enseignant

<h2>Supprimer Enseignant</h2>
<p>Êtes-vous sûr de vouloir supprimer l'enseignant <strong>@Model.Nom @Model.Prenom</strong> ?</p>

<form asp-action="Delete" asp-route-id="@Model.CodeEnseignant" method="post">
    <input type="hidden" asp-for="CodeEnseignant" />
    <button type="submit" class="btn btn-danger">Supprimer</button>
    <a asp-action="Index" class="btn btn-secondary">Annuler</a>
</form>

